{
  "Logging": {
    "LogLevel": {
      "Default": "Error",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Kestrel": {
    "Endpoints": {
      "Http": {
        "Url": "http://localhost:5000"
      },
      "HttpsInlineCertAndKeyFile": {
        "Url": "https://localhost:5001",
        "Certificate": {
          "AllowInvalid": true,
          "KeyPath": "/etc/letsencrypt/live/flocon.io/privkey.pem",
          "Path": "/etc/letsencrypt/live/flocon.io/fullchain.pem"
        }
      }
    }
  }
}
